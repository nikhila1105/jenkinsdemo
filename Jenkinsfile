pipeline {
  agent any
  
  stages {
    stage('Hello') {
      steps {
        echo 'hello w'
        bat 'mvn test'
      }
    }
    stage('Build') {
      steps{
        echo 'building'
      }
    }
    stage('Deploy') {
      steps{
        echo 'deploying'
      }
    }
    stage('Test') {
      steps{
        echo 'testing'
      }
    }
    stage('Release') {
      steps{
        echo 'releasing'
      }
    }
  }
}
    
      
    
        
      
  
